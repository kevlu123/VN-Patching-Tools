[
    {
        "offset": 1246,
        "size": 42,
        "name_offset": 1208,
        "name": "【あげは】",
        "jp": "「小鳥～～～～～～～！！！」",
        "text": ""
    },
    {
        "offset": 1954,
        "size": 42,
        "name_offset": 1919,
        "name": "【小鳥】",
        "jp": "「あげはーーーーーー！！！」",
        "text": ""
    },
    {
        "offset": 2620,
        "size": 81,
        "name_offset": 0,
        "name": "",
        "jp": "再会した親友同士が、キッチンの真ん中でひしと抱き合う。",
        "text": ""
    },
    {
        "offset": 3180,
        "size": 51,
        "name_offset": 3142,
        "name": "【あげは】",
        "jp": "「ひさしぶりだねぇ、元気だった？」",
        "text": ""
    },
    {
        "offset": 3526,
        "size": 30,
        "name_offset": 3491,
        "name": "【小鳥】",
        "jp": "「うん、元気元気！」",
        "text": ""
    },
    {
        "offset": 4403,
        "size": 69,
        "name_offset": 4368,
        "name": "【亜紗】",
        "jp": "「わぁ～、せんぱいたちがトビウオ荘にいるよぉ」",
        "text": ""
    },
    {
        "offset": 4737,
        "size": 78,
        "name_offset": 4702,
        "name": "【依瑠】",
        "jp": "「夏休みで静かだったのが、一気に騒がしくなったわね」",
        "text": ""
    },
    {
        "offset": 5019,
        "size": 91,
        "name_offset": 4984,
        "name": "【小鳥】",
        "jp": "「ごきげんよう、ふたごシスターズ。相変わらずそっくりね」",
        "text": ""
    },
    {
        "offset": 6043,
        "size": 121,
        "name_offset": 6005,
        "name": "【あげは】",
        "jp": "「天音先輩、お久しぶりです。ご活躍はかねがね、テレビとかで拝見しております」",
        "text": ""
    },
    {
        "offset": 6459,
        "size": 109,
        "name_offset": 6424,
        "name": "【天音】",
        "jp": "「もぐもぐ……うん、久しぶりだなぁ。元気そうでなにより……もぐもぐ」",
        "text": ""
    },
    {
        "offset": 6836,
        "size": 48,
        "name_offset": 6798,
        "name": "【イスカ】",
        "jp": "「あんた、いつまで食べてるのよ」",
        "text": ""
    },
    {
        "offset": 7675,
        "size": 39,
        "name_offset": 7643,
        "name": "【碧】",
        "jp": "「霧乃さんはどうしたの？」",
        "text": ""
    },
    {
        "offset": 7979,
        "size": 48,
        "name_offset": 7944,
        "name": "【亜紗】",
        "jp": "「夏休みなので、家に帰ってます」",
        "text": ""
    },
    {
        "offset": 8322,
        "size": 57,
        "name_offset": 8287,
        "name": "【依瑠】",
        "jp": "「寮に残ってるのは、わたしらだけです」",
        "text": ""
    },
    {
        "offset": 8537,
        "size": 168,
        "name_offset": 0,
        "name": "",
        "jp": "そうか。霧乃さんや晴海にも会いたかったが。まあ、休暇は今日だけじゃないから、予定が合えば顔くらい見られるだろう。",
        "text": ""
    },
    {
        "offset": 9552,
        "size": 54,
        "name_offset": 9517,
        "name": "【小鳥】",
        "jp": "「ねえねえあげは、水着どうしたの？」",
        "text": ""
    },
    {
        "offset": 9904,
        "size": 76,
        "name_offset": 9866,
        "name": "【あげは】",
        "jp": "「去年のがキツくなってたから、新しいの買った」",
        "text": ""
    },
    {
        "offset": 10755,
        "size": 63,
        "name_offset": 10720,
        "name": "【小鳥】",
        "jp": "「むむ……確かに、また少し成長してるわね」",
        "text": ""
    },
    {
        "offset": 10976,
        "size": 99,
        "name_offset": 0,
        "name": "",
        "jp": "小鳥があげはの胸元をじとっと見つめながら言う。心なしか羨ましげだ。",
        "text": ""
    },
    {
        "offset": 11915,
        "size": 30,
        "name_offset": 11877,
        "name": "【あげは】",
        "jp": "「そういう小鳥は？」",
        "text": ""
    },
    {
        "offset": 12278,
        "size": 39,
        "name_offset": 12243,
        "name": "【小鳥】",
        "jp": "「わ、私はね……まだ秘密」",
        "text": ""
    },
    {
        "offset": 12615,
        "size": 36,
        "name_offset": 12577,
        "name": "【あげは】",
        "jp": "「えー、教えなさいよー」",
        "text": ""
    },
    {
        "offset": 12984,
        "size": 60,
        "name_offset": 12949,
        "name": "【小鳥】",
        "jp": "「うー、じゃあこっそり見せっこしよっか」",
        "text": ""
    },
    {
        "offset": 13342,
        "size": 39,
        "name_offset": 13304,
        "name": "【あげは】",
        "jp": "「碧ー、脱衣所借りるねー」",
        "text": ""
    },
    {
        "offset": 14042,
        "size": 51,
        "name_offset": 14010,
        "name": "【碧】",
        "jp": "「いや、俺に言われても困るんだが」",
        "text": ""
    },
    {
        "offset": 14251,
        "size": 30,
        "name_offset": 0,
        "name": "",
        "jp": "もう寮母じゃないし。",
        "text": ""
    },
    {
        "offset": 15076,
        "size": 24,
        "name_offset": 15038,
        "name": "【イスカ】",
        "jp": "「なになにー？」",
        "text": ""
    },
    {
        "offset": 15395,
        "size": 42,
        "name_offset": 15360,
        "name": "【亜紗】",
        "jp": "「水着の見せっこするみたい」",
        "text": ""
    },
    {
        "offset": 16047,
        "size": 45,
        "name_offset": 16012,
        "name": "【依瑠】",
        "jp": "「先輩はのぞいちゃダメですよ」",
        "text": ""
    },
    {
        "offset": 16492,
        "size": 24,
        "name_offset": 16460,
        "name": "【碧】",
        "jp": "「やれやれ……」",
        "text": ""
    },
    {
        "offset": 16674,
        "size": 57,
        "name_offset": 0,
        "name": "",
        "jp": "この女所帯に囲まれてる感じも懐かしい。",
        "text": ""
    },
    {
        "offset": 17498,
        "size": 48,
        "name_offset": 17463,
        "name": "【天音】",
        "jp": "「？　みんなどこ行ったんだー？」",
        "text": ""
    },
    {
        "offset": 17747,
        "size": 179,
        "name_offset": 17715,
        "name": "【碧】",
        "jp": "「ここに色気より食い気の人が。どうぞ先輩は、朝食の続きを堪能してください。今、コーヒーのおかわり入れますから」",
        "text": ""
    },
    {
        "offset": 18191,
        "size": 18,
        "name_offset": 18156,
        "name": "【天音】",
        "jp": "「わーい♪」",
        "text": ""
    },
    {
        "offset": 19067,
        "size": 108,
        "name_offset": 0,
        "name": "",
        "jp": "今日はバカンス。元ソアリング部のメンバーと、夏を満喫するために集まった。",
        "text": ""
    },
    {
        "offset": 19333,
        "size": 36,
        "name_offset": 0,
        "name": "",
        "jp": "となれば、やる事は一つ。",
        "text": ""
    },
    {
        "offset": 20010,
        "size": 84,
        "name_offset": 0,
        "name": "",
        "jp": "湖の片隅。崖に囲まれて周囲からは見えない、秘密のビーチ。",
        "text": ""
    },
    {
        "offset": 20252,
        "size": 102,
        "name_offset": 0,
        "name": "",
        "jp": "恵風学園にいた頃から、夏休みになるとここへ泳ぎに来るのが定番だった。",
        "text": ""
    }
]