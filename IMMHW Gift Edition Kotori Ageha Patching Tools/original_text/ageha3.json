[
    {
        "offset": 597,
        "size": 118,
        "name_offset": 559,
        "name": "【あげは】",
        "jp": "「焼けるような真夏の太陽、きらめく汗。いやーー絶好の湖水浴日和ですなーー」",
        "text": ""
    },
    {
        "offset": 916,
        "size": 18,
        "name_offset": 884,
        "name": "【碧】",
        "jp": "「そうだな」",
        "text": ""
    },
    {
        "offset": 1428,
        "size": 69,
        "name_offset": 1390,
        "name": "【あげは】",
        "jp": "「おや？　さっきから何そわそわしてるのかな？」",
        "text": ""
    },
    {
        "offset": 1698,
        "size": 54,
        "name_offset": 1666,
        "name": "【碧】",
        "jp": "「べ、別にそわそわなんかしてねーし」",
        "text": ""
    },
    {
        "offset": 2050,
        "size": 109,
        "name_offset": 2012,
        "name": "【あげは】",
        "jp": "「ははぁ、さっきトビウオ荘でお預けくらったから、待ち遠しいんだ～♪」",
        "text": ""
    },
    {
        "offset": 2317,
        "size": 108,
        "name_offset": 0,
        "name": "",
        "jp": "女子だけで水着の見せ合いをしていたのが、羨ましかったなんて事は全然ない。",
        "text": ""
    },
    {
        "offset": 2723,
        "size": 72,
        "name_offset": 2685,
        "name": "【あげは】",
        "jp": "「姫城あげはちゃんの、今年の夏の新作水着は～～」",
        "text": ""
    },
    {
        "offset": 3014,
        "size": 84,
        "name_offset": 0,
        "name": "",
        "jp": "あげはが口でドラムロールをしながら、いきなり――ガバッ！",
        "text": ""
    },
    {
        "offset": 3667,
        "size": 48,
        "name_offset": 0,
        "name": "",
        "jp": "その場で勢いよく服を脱ぎ捨てた。",
        "text": ""
    },
    {
        "offset": 3916,
        "size": 21,
        "name_offset": 3884,
        "name": "【碧】",
        "jp": "「お、おいっ」",
        "text": ""
    },
    {
        "offset": 5315,
        "size": 30,
        "name_offset": 5283,
        "name": "【碧】",
        "jp": "「……って、あれ？」",
        "text": ""
    },
    {
        "offset": 5552,
        "size": 42,
        "name_offset": 5514,
        "name": "【あげは】",
        "jp": "「下に着てきてたんだよ～ん」",
        "text": ""
    },
    {
        "offset": 5801,
        "size": 48,
        "name_offset": 5763,
        "name": "【あげは】",
        "jp": "「にひひ、なーにびびってんの？」",
        "text": ""
    },
    {
        "offset": 6050,
        "size": 39,
        "name_offset": 6018,
        "name": "【碧】",
        "jp": "「べ、別にびびってねーし」",
        "text": ""
    },
    {
        "offset": 6467,
        "size": 69,
        "name_offset": 6432,
        "name": "【依瑠】",
        "jp": "「おもいっきりびっくりした顔してましたけどね」",
        "text": ""
    },
    {
        "offset": 6740,
        "size": 51,
        "name_offset": 6705,
        "name": "【亜紗】",
        "jp": "「わ……わたしもドキドキした～！」",
        "text": ""
    },
    {
        "offset": 7172,
        "size": 43,
        "name_offset": 7134,
        "name": "【あげは】",
        "jp": "「ねえ、碧。……どう？」",
        "text": ""
    },
    {
        "offset": 7416,
        "size": 21,
        "name_offset": 7384,
        "name": "【碧】",
        "jp": "「どうって？」",
        "text": ""
    },
    {
        "offset": 7644,
        "size": 24,
        "name_offset": 7606,
        "name": "【あげは】",
        "jp": "「だからぁ……」",
        "text": ""
    },
    {
        "offset": 7872,
        "size": 33,
        "name_offset": 7837,
        "name": "【依瑠】",
        "jp": "「にぶいですね、先輩」",
        "text": ""
    },
    {
        "offset": 8063,
        "size": 63,
        "name_offset": 0,
        "name": "",
        "jp": "いや分かってるよ。ただ、照れくさいだけで。",
        "text": ""
    },
    {
        "offset": 8327,
        "size": 33,
        "name_offset": 8295,
        "name": "【碧】",
        "jp": "「すげー似合ってるぞ」",
        "text": ""
    },
    {
        "offset": 8738,
        "size": 82,
        "name_offset": 8703,
        "name": "【亜紗】",
        "jp": "「似合ってるですって！よかったですね、あげは先輩」",
        "text": ""
    },
    {
        "offset": 9024,
        "size": 97,
        "name_offset": 8989,
        "name": "【依瑠】",
        "jp": "「まあ、これを見てときめかない男性はいないんじゃないかしら」",
        "text": ""
    },
    {
        "offset": 9502,
        "size": 100,
        "name_offset": 9464,
        "name": "【あげは】",
        "jp": "「えへへ、やっぱり嬉しいね。面と向かって褒めてもらえるのって」",
        "text": ""
    },
    {
        "offset": 9809,
        "size": 57,
        "name_offset": 9771,
        "name": "【あげは】",
        "jp": "「幼なじみ同士だとね、照れちゃうから」",
        "text": ""
    },
    {
        "offset": 10024,
        "size": 42,
        "name_offset": 0,
        "name": "",
        "jp": "あげははそう言ってはにかむ。",
        "text": ""
    },
    {
        "offset": 10336,
        "size": 51,
        "name_offset": 10298,
        "name": "【あげは】",
        "jp": "「さぁ、いこ！　ほらほら、早く～」",
        "text": ""
    },
    {
        "offset": 10718,
        "size": 63,
        "name_offset": 0,
        "name": "",
        "jp": "ビーチボールを持って、湖へ駆け込んでいく。",
        "text": ""
    },
    {
        "offset": 11216,
        "size": 18,
        "name_offset": 11178,
        "name": "【あげは】",
        "jp": "「それそれ」",
        "text": ""
    },
    {
        "offset": 11435,
        "size": 18,
        "name_offset": 11403,
        "name": "【碧】",
        "jp": "「やったな」",
        "text": ""
    },
    {
        "offset": 11611,
        "size": 96,
        "name_offset": 0,
        "name": "",
        "jp": "じゃれ合うように水をかけあって遊んでいると、子供の頃を思い出す。",
        "text": ""
    },
    {
        "offset": 11865,
        "size": 72,
        "name_offset": 0,
        "name": "",
        "jp": "あんちゃんやみんなと、よくこうして水遊びをした。",
        "text": ""
    },
    {
        "offset": 12499,
        "size": 60,
        "name_offset": 12467,
        "name": "【碧】",
        "jp": "「いやでも、もう全然子供じゃないな……」",
        "text": ""
    },
    {
        "offset": 12766,
        "size": 51,
        "name_offset": 12728,
        "name": "【あげは】",
        "jp": "「どこ見て言ってんのよ、えっち♪」",
        "text": ""
    },
    {
        "offset": 12975,
        "size": 156,
        "name_offset": 0,
        "name": "",
        "jp": "いたずらっぽく笑った笑顔はあの頃のまんまで、真夏の陽射しと相まって、キラキラと輝いて眩しいくらいだった。",
        "text": ""
    },
    {
        "offset": 13491,
        "size": 54,
        "name_offset": 0,
        "name": "",
        "jp": "今年の夏休みも、熱くなりそうだった。",
        "text": ""
    }
]