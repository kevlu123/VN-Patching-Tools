[
    {
        "offset": 1349,
        "size": 51,
        "name_offset": 1317,
        "name": "【碧】",
        "jp": "「……帰ってきたって感じがするな」",
        "text": ""
    },
    {
        "offset": 1558,
        "size": 57,
        "name_offset": 0,
        "name": "",
        "jp": "１年ぶりに立つ、トビウオ荘のキッチン。",
        "text": ""
    },
    {
        "offset": 1773,
        "size": 39,
        "name_offset": 0,
        "name": "",
        "jp": "懐かしさがこみ上げてくる。",
        "text": ""
    },
    {
        "offset": 1970,
        "size": 180,
        "name_offset": 0,
        "name": "",
        "jp": "ここで毎日、寮生の食事を作っていた。女子寮という、男にとってアウェイな環境で、ここだけは寮母である俺のテリトリーだった。",
        "text": ""
    },
    {
        "offset": 2351,
        "size": 45,
        "name_offset": 2319,
        "name": "【碧】",
        "jp": "「感慨に浸ってる時間はないぞ」",
        "text": ""
    },
    {
        "offset": 2554,
        "size": 39,
        "name_offset": 0,
        "name": "",
        "jp": "さっそく仕事に取りかかる。",
        "text": ""
    },
    {
        "offset": 2751,
        "size": 108,
        "name_offset": 0,
        "name": "",
        "jp": "今日のバカンスのため、みんなの弁当をここで作らせてもらう事になっていた。",
        "text": ""
    },
    {
        "offset": 3017,
        "size": 147,
        "name_offset": 0,
        "name": "",
        "jp": "もちろん、許可はもらっている。ほとんどの寮生は帰省しているので、男の俺が入っても問題はないそうだ。",
        "text": ""
    },
    {
        "offset": 3541,
        "size": 42,
        "name_offset": 0,
        "name": "",
        "jp": "てきぱきと調理を進めていく。",
        "text": ""
    },
    {
        "offset": 4021,
        "size": 18,
        "name_offset": 3983,
        "name": "【ハット】",
        "jp": "「ガァガァ」",
        "text": ""
    },
    {
        "offset": 4240,
        "size": 91,
        "name_offset": 4208,
        "name": "【碧】",
        "jp": "「なんだ腹が減ってるのか？しょうがないな、少しだけだぞ」",
        "text": ""
    },
    {
        "offset": 4521,
        "size": 48,
        "name_offset": 0,
        "name": "",
        "jp": "材料のキュウリを切り分けてやる。",
        "text": ""
    },
    {
        "offset": 4837,
        "size": 18,
        "name_offset": 4799,
        "name": "【ハット】",
        "jp": "「ガフガフ」",
        "text": ""
    },
    {
        "offset": 5123,
        "size": 21,
        "name_offset": 5085,
        "name": "【ハット】",
        "jp": "「……グワ？」",
        "text": ""
    },
    {
        "offset": 5302,
        "size": 69,
        "name_offset": 0,
        "name": "",
        "jp": "美味そうに食べていたハットが、急に首を上げた。",
        "text": ""
    },
    {
        "offset": 5572,
        "size": 27,
        "name_offset": 5540,
        "name": "【碧】",
        "jp": "「どうしたんだ？」",
        "text": ""
    },
    {
        "offset": 6009,
        "size": 18,
        "name_offset": 5971,
        "name": "【ハット】",
        "jp": "「ガァガァ」",
        "text": ""
    },
    {
        "offset": 6349,
        "size": 36,
        "name_offset": 6317,
        "name": "【碧】",
        "jp": "「おい、どこいくんだっ」",
        "text": ""
    },
    {
        "offset": 6965,
        "size": 111,
        "name_offset": 0,
        "name": "",
        "jp": "ハットが居なくなってすぐ、聞き覚えのあるエンジン音が、外から聞こえてきた。",
        "text": ""
    },
    {
        "offset": 7813,
        "size": 21,
        "name_offset": 7775,
        "name": "【ハット】",
        "jp": "「ガァガァ！」",
        "text": ""
    },
    {
        "offset": 8495,
        "size": 24,
        "name_offset": 8448,
        "name": "【麗しの美少女】",
        "jp": "「あら、ハット」",
        "text": ""
    },
    {
        "offset": 8796,
        "size": 79,
        "name_offset": 8749,
        "name": "【麗しの美少女】",
        "jp": "「どうしたのあんた。お出迎えしに来てくれたの？」",
        "text": ""
    },
    {
        "offset": 9082,
        "size": 30,
        "name_offset": 9044,
        "name": "【ハット】",
        "jp": "「クワァ～～～～♪」",
        "text": ""
    },
    {
        "offset": 9389,
        "size": 55,
        "name_offset": 9342,
        "name": "【麗しの美少女】",
        "jp": "「ふふふ、よしよし。えらいわね」",
        "text": ""
    },
    {
        "offset": 9645,
        "size": 12,
        "name_offset": 9613,
        "name": "【碧】",
        "jp": "「小鳥」",
        "text": ""
    },
    {
        "offset": 9934,
        "size": 9,
        "name_offset": 9887,
        "name": "【麗しの美少女】",
        "jp": "「？」",
        "text": ""
    },
    {
        "offset": 10101,
        "size": 66,
        "name_offset": 0,
        "name": "",
        "jp": "アヒルと戯れていた麗しの美少女が顔をあげた。",
        "text": ""
    },
    {
        "offset": 10968,
        "size": 9,
        "name_offset": 10933,
        "name": "【小鳥】",
        "jp": "「碧」",
        "text": ""
    },
    {
        "offset": 11395,
        "size": 78,
        "name_offset": 0,
        "name": "",
        "jp": "美少女はアヒルを放り捨てて、俺の方へ駆け寄ってきた。",
        "text": ""
    },
    {
        "offset": 11786,
        "size": 15,
        "name_offset": 11751,
        "name": "【小鳥】",
        "jp": "「っとと」",
        "text": ""
    },
    {
        "offset": 12334,
        "size": 15,
        "name_offset": 12302,
        "name": "【碧】",
        "jp": "「おっと」",
        "text": ""
    },
    {
        "offset": 12507,
        "size": 93,
        "name_offset": 0,
        "name": "",
        "jp": "杖をつきながら走ったせいで、転びそうになった小鳥を抱き留めた。",
        "text": ""
    },
    {
        "offset": 12865,
        "size": 27,
        "name_offset": 12830,
        "name": "【小鳥】",
        "jp": "「ナイスキャッチ」",
        "text": ""
    },
    {
        "offset": 13050,
        "size": 69,
        "name_offset": 0,
        "name": "",
        "jp": "小鳥はそのまま俺に体重を預けて抱きついてきた。",
        "text": ""
    },
    {
        "offset": 13384,
        "size": 33,
        "name_offset": 13349,
        "name": "【小鳥】",
        "jp": "「ぬふふ～～すりすり」",
        "text": ""
    },
    {
        "offset": 13911,
        "size": 60,
        "name_offset": 0,
        "name": "",
        "jp": "停まっていた車がクラクションを鳴らした。",
        "text": ""
    },
    {
        "offset": 14355,
        "size": 45,
        "name_offset": 0,
        "name": "",
        "jp": "運転席の女性が窓から顔を出す。",
        "text": ""
    },
    {
        "offset": 14735,
        "size": 76,
        "name_offset": 14697,
        "name": "【ひばり】",
        "jp": "「じゃあね小鳥ちゃん。バカンス楽しんで来てね」",
        "text": ""
    },
    {
        "offset": 15015,
        "size": 45,
        "name_offset": 14980,
        "name": "【小鳥】",
        "jp": "「うん、ありがとうお姉ちゃん」",
        "text": ""
    },
    {
        "offset": 15474,
        "size": 132,
        "name_offset": 0,
        "name": "",
        "jp": "小鳥を抱き留めたまま会釈すると、ひばりさんも会釈を返して、手を振ってから車を発進させた。",
        "text": ""
    },
    {
        "offset": 16131,
        "size": 69,
        "name_offset": 16099,
        "name": "【碧】",
        "jp": "「ひばりさんも、少し休んでいけばよかったのに」",
        "text": ""
    },
    {
        "offset": 16465,
        "size": 66,
        "name_offset": 16430,
        "name": "【小鳥】",
        "jp": "「みんなの水入らずを邪魔したくないんだって」",
        "text": ""
    },
    {
        "offset": 16689,
        "size": 57,
        "name_offset": 0,
        "name": "",
        "jp": "相変わらず甲斐甲斐しいお姉さんだった。",
        "text": ""
    },
    {
        "offset": 17739,
        "size": 33,
        "name_offset": 17704,
        "name": "【小鳥】",
        "jp": "「みんなはまだなの？」",
        "text": ""
    },
    {
        "offset": 17973,
        "size": 109,
        "name_offset": 17941,
        "name": "【碧】",
        "jp": "「俺が一番乗りで、次が小鳥かな。あげはは近所だし、そのうち来るだろ」",
        "text": ""
    },
    {
        "offset": 18347,
        "size": 66,
        "name_offset": 18312,
        "name": "【小鳥】",
        "jp": "「天音ちゃんからは結局、返事がなかったわね」",
        "text": ""
    },
    {
        "offset": 18614,
        "size": 185,
        "name_offset": 18582,
        "name": "【碧】",
        "jp": "「まあ、あの人は忙しいから。今もアメリカにいるのか日本にいるのか、はたまた地球の裏側にいるのかも分からんからなぁ」",
        "text": ""
    },
    {
        "offset": 19094,
        "size": 85,
        "name_offset": 19059,
        "name": "【小鳥】",
        "jp": "「くんくん……いい匂いがする。これはタマゴ焼きね！」",
        "text": ""
    },
    {
        "offset": 19380,
        "size": 57,
        "name_offset": 19348,
        "name": "【碧】",
        "jp": "「ご明察。みんなの弁当を作ってたんだ」",
        "text": ""
    },
    {
        "offset": 19732,
        "size": 130,
        "name_offset": 19697,
        "name": "【小鳥】",
        "jp": "「わーい、碧のタマゴ焼きはふわっふわで、ちょっぴりダシが効いてて美味しいのよね～」",
        "text": ""
    },
    {
        "offset": 20458,
        "size": 158,
        "name_offset": 20423,
        "name": "【天音】",
        "jp": "「はぐはぐ、もぐもぐ……うむ、そうか、ダシが効いてるから美味しいのか。なるほどなぁ……もぐもぐ」",
        "text": ""
    },
    {
        "offset": 21195,
        "size": 18,
        "name_offset": 21160,
        "name": "【小鳥】",
        "jp": "「…………」",
        "text": ""
    },
    {
        "offset": 21802,
        "size": 51,
        "name_offset": 21767,
        "name": "【小鳥】",
        "jp": "「……って、天音ちゃんっっ！！？」",
        "text": ""
    },
    {
        "offset": 22478,
        "size": 88,
        "name_offset": 22443,
        "name": "【天音】",
        "jp": "「やぁ、小鳥、碧くん。先にいただいてるよ……もぐもぐ」",
        "text": ""
    },
    {
        "offset": 23013,
        "size": 124,
        "name_offset": 22978,
        "name": "【小鳥】",
        "jp": "「って、コラー！　それはみんなのお昼ご飯でしょうがっ！今食べちゃダメなヤツ！」",
        "text": ""
    },
    {
        "offset": 23432,
        "size": 24,
        "name_offset": 23397,
        "name": "【天音】",
        "jp": "「そうなのか？」",
        "text": ""
    },
    {
        "offset": 23751,
        "size": 103,
        "name_offset": 23716,
        "name": "【天音】",
        "jp": "「いい匂いだったからつい。朝ご飯じゃなかったんだな……もぐもぐ」",
        "text": ""
    },
    {
        "offset": 24301,
        "size": 39,
        "name_offset": 24266,
        "name": "【小鳥】",
        "jp": "「そう言いながら食うなっ」",
        "text": ""
    },
    {
        "offset": 25009,
        "size": 36,
        "name_offset": 24971,
        "name": "【イスカ】",
        "jp": "「なになにどうしたの？」",
        "text": ""
    },
    {
        "offset": 25246,
        "size": 48,
        "name_offset": 25214,
        "name": "【碧】",
        "jp": "「あ、イスカさん、いらっしゃい」",
        "text": ""
    },
    {
        "offset": 26037,
        "size": 109,
        "name_offset": 25999,
        "name": "【イスカ】",
        "jp": "「天音、あんたまた盗み食いしたの？朝ご飯、来る前に食べたでしょうが」",
        "text": ""
    },
    {
        "offset": 26441,
        "size": 45,
        "name_offset": 26406,
        "name": "【天音】",
        "jp": "「……いい匂いだったからつい」",
        "text": ""
    },
    {
        "offset": 26687,
        "size": 103,
        "name_offset": 26655,
        "name": "【碧】",
        "jp": "「タマゴ焼きなら、材料も余ってるし、すぐ作れるんで大丈夫ですよ」",
        "text": ""
    },
    {
        "offset": 27085,
        "size": 39,
        "name_offset": 27050,
        "name": "【小鳥】",
        "jp": "「やれやれだわ、ほんとに」",
        "text": ""
    },
    {
        "offset": 27282,
        "size": 120,
        "name_offset": 0,
        "name": "",
        "jp": "小鳥もプンプン怒った顔はしつつも、天音先輩が来てくれた事を喜んでるみたいだった。",
        "text": ""
    }
]